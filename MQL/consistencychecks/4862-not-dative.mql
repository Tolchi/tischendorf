SELECT ALL OBJECTS
WHERE
[word FOCUS strongs1 = 4862] // SU/N
[word FOCUS case <> dative
	AND psp IN (noun, article, adjective, 
		    correlative_or_interrogative_pronoun, 
		    correlative_pronoun, demonstrative_pronoun, 
                    indefinite_pronoun, personal_pronoun, 
                    possessive_pronoun, reciprocal_pronoun, 
                    reflexive_pronoun, relative_pronoun)]
GO


